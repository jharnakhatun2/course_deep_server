{
  "version": 2,
  "builds": [
    {
      "src": "app/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(api|auth|users|courses|instructorcourse|events|bookings|enrollments|blogs|comments|reviews|payments|cart|health)/.*",
      "dest": "app/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|wasm|mjs))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}